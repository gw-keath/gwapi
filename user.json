          "response": [
            {
              "name": "成功發送測試郵件",
              "status": 200,
              "description": "成功發送測試郵件",
              "body": "{\"message\":\"Email sent successfully!\"}"
            },
            {
              "name": "請求驗證失敗",
              "status": 422,
              "description": "輸入的數據驗證失敗，通常是電子郵件格式不正確",
              "body": "{\"message\":\"The given data was invalid.\",\"errors\":{\"to\":[\"The to field is required.\"]}}"
            },
            {
              "name": "發送失敗",
              "status": 500,
              "description": "伺服器內部錯誤導致郵件未成功發送",
              "body": "{\"error\":\"Failed to send email.\",\"details\":\"SMTP connection failed.\"}"
            }
          ]
        }
      ]
    },
