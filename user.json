  "item": [
    {
      "name": "Users",
      "item": [
        {
          "name": "Get all users",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/users",
              "host": ["{{base_url}}"],
              "path": ["api", "users"]
            }
          },
          "response": [
            {
              "name": "成功獲取所有使用者",
              "status": 200,
              "description": "成功獲取所有使用者",
              "body": "[{\"id\":1,\"name\":\"John\",\"email\":\"john@example.com\"},...]"
            }
          ]
        },
        {
          "name": "Get a user by ID",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/users/:id",
              "host": ["{{base_url}}"],
              "path": ["api", "users", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "1"
                }
              ]
            }
          },
          "response": [
            {
              "name": "成功獲取單個使用者",
              "status": 200,
              "description": "成功獲取單個使用者",
              "body": "{\"id\":1,\"name\":\"John\",\"email\":\"john@example.com\"}"
            },
            {
              "name": "使用者不存在",
              "status": 404,
              "description": "使用者不存在",
              "body": "{\"message\": \"User not found\"}"
            }
          ]
        },
        {
          "name": "Create a new user",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\"name\":\"John\",\"email\":\"john@example.com\",\"password\":\"password123\"}"
            },
            "url": {
              "raw": "{{base_url}}/api/users",
              "host": ["{{base_url}}"],
              "path": ["api", "users"]
            }
          },
          "response": [
            {
              "name": "成功創建新使用者",
              "status": 201,
              "description": "成功創建新使用者",
              "body": "{\"id\":1,\"name\":\"John\",\"email\":\"john@example.com\"}"
            },
            {
              "name": "Email 已存在",
              "status": 409,
              "description": "Email 已存在，無法創建新用戶",
              "body": "{\"message\":\"User with this email already exists\"}"
            },
            {
              "name": "請求驗證失敗",
              "status": 422,
              "description": "請求驗證失敗",
              "body": "{\"message\":\"The given data was invalid.\",\"errors\":{\"name\":[\"The name field is required.\"],\"email\":[\"The email field is required.\"],\"password\":[\"The password field is required.\"]}}"
            }
          ]
        },
        {
          "name": "Update an existing user",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\"name\":\"John Updated\",\"email\":\"john.updated@example.com\",\"password\":\"newpassword123\"}"
            },
            "url": {
              "raw": "{{base_url}}/api/users/:id",
              "host": ["{{base_url}}"],
              "path": ["api", "users", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "1"
                }
              ]
            }
          },
          "response": [
            {
              "name": "成功更新使用者",
              "status": 200,
              "description": "成功更新使用者",
              "body": "{\"id\":1,\"name\":\"John Updated\",\"email\":\"john.updated@example.com\"}"
            },
            {
              "name": "使用者不存在",
              "status": 404,
              "description": "使用者不存在",
              "body": "{\"message\": \"User not found\"}"
            },
            {
              "name": "請求驗證失敗",
              "status": 422,
              "description": "請求驗證失敗",
              "body": "{\"message\":\"The given data was invalid.\",\"errors\":{\"email\":[\"The email must be a valid email address.\"]}}"
            }
          ]
        },
        {
          "name": "Delete a user by ID",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{base_url}}/api/users/:id",
              "host": ["{{base_url}}"],
              "path": ["api", "users", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "1"
                }
              ]
            }
          },
          "response": [
            {
              "name": "成功刪除使用者",
              "status": 200,
              "description": "成功刪除使用者",
              "body": "{\"message\": \"User deleted successfully\"}"
            },
            {
              "name": "使用者不存在",
              "status": 404,
              "description": "使用者不存在",
              "body": "{\"message\": \"User not found\"}"
            }
          ]
        }
      ]
    },
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register a new user",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\"name\":\"John\",\"email\":\"john@example.com\",\"password\":\"password123\",\"password_confirmation\":\"password123\"}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/register",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "register"]
            }
          },
          "response": [
            {
              "name": "成功註冊用戶",
              "status": 201,
              "description": "用戶註冊成功並發送驗證信。",
              "body": "{\"message\":\"Registration successful. Please check your email to verify your account.\"}"
            },
            {
              "name": "驗證失敗",
              "status": 422,
              "description": "註冊時的資料驗證失敗。",
              "body": "{\"message\":\"The given data was invalid.\",\"errors\":{\"password_confirmation\":[\"The password confirmation does not match.\"]}}"
            }
          ]
        },
        {
          "name": "User login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\"email\":\"john@example.com\",\"password\":\"password123\"}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/login",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "login"]
            }
          },
          "response": [
            {
              "name": "登入成功",
              "status": 200,
              "description": "用戶成功登入。",
              "body": "{\"user\":{\"id\":1,\"name\":\"John\",\"email\":\"john@example.com\"},\"access_token\":\"token\",\"token_type\":\"Bearer\"}"
            },
            {
              "name": "登入失敗",
              "status": 401,
              "description": "登入失敗，憑證無效。",
              "body": "{\"message\": \"Invalid login credentials\"}"
            }            
          ]
        },
        {
          "name": "User logout",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/auth/logout",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "logout"]
            }
          },
          "response": [
            {
              "name": "成功登出",
              "status": 200,
              "description": "用戶成功登出。",
              "body": "{\"message\": \"Successfully logged out\"}"
            },
            {
              "name": "未授權",
              "status": 401,
              "description": "無效的 Token 或未認證。",
              "body": "{\"message\": \"Unauthorized\"}"
            }
          ]
        },
        {
          "name": "Get current user",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/auth/user",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "user"]
            }
          },
          "response": [
            {
              "name": "成功獲取用戶資料",
              "status": 200,
              "description": "成功獲取當前用戶的資料。",
              "body": "{\"id\":1,\"name\":\"John\",\"email\":\"john@example.com\"}"
            },
            {
              "name": "未授權",
              "status": 401,
              "description": "用戶未經認證。",
              "body": "{\"message\": \"Unauthorized\"}"
            }
          ]
        },
        {
          "name": "Verify email address",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/auth/email/verify/1/abcdef123456",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "email", "verify", "1", "abcdef123456"]
            }
          },
          "response": [
            {
              "name": "成功驗證電子郵件",
              "status": 200,
              "description": "電子郵件驗證成功。",
              "body": "{\"message\": \"Email verification successful.\"}"
            },
            {
              "name": "用戶不存在",
              "status": 404,
              "description": "無法找到對應的用戶。",
              "body": "{\"message\": \"User not found.\"}"
            },
            {
              "name": "驗證連結無效",
              "status": 403,
              "description": "驗證連結無效。",
              "body": "{\"message\": \"Invalid verification link.\"}"
            },
            {
              "name": "驗證連結已過期",
              "status": 403,
              "description": "驗證連結無效或已過期。",
              "body": "{\"message\": \"Invalid or expired verification link.\"}"
            },
            {
              "name": "電子郵件已驗證",
              "status": 200,
              "description": "電子郵件已經被驗證過。",
              "body": "{\"message\": \"Email is already verified.\"}"
            }
          ]
        },
        {
          "name": "Resend verification email",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/auth/email/resend",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "email", "resend"]
            }
          },
          "response": [
            {
              "name": "成功重新發送驗證郵件",
              "status": 200,
              "description": "驗證郵件重新發送成功。",
              "body": "{\"message\": \"Verification email resent successfully.\"}"
            },
            {
              "name": "未授權",
              "status": 401,
              "description": "用戶未經認證。",
              "body": "{\"message\": \"Unauthorized\"}"
            }
          ]
        }
      ]
    },
    {
      "name": "Email",
      "item": [
        {
          "name": "Send Test Email",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
